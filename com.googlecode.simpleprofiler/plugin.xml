<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="com.googlecode.simpleprofiler.instrumentjavaproject"
            objectClass="org.eclipse.jdt.core.IJavaProject">
         <action
               class="com.googlecode.simpleprofiler.action.InstrumentProjectAction"
               enablesFor="*"
               id="com.googlecode.simpleprofiler.instrumentaction"
               label="Instrument"
               menubarPath="additions">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="com.googlecode.simpleprofiler.test"
            label="testAction"
            visible="true">
         <action
               class="com.googlecode.simpleprofiler.action.TestAction"
               id="com.googlecode.simpleprofiler.action1"
               label="TestAction"
               style="push"
               toolbarPath="additions">
         </action>
      </actionSet>
   </extension>

</plugin>
